public class App {
	public static void main(String[] args){
		byte byteValue = 20;
		short shortValue = 55;
		int intVal = 666;
		long longValue = 233489;
		
		float floatVal = 234.5f; // add f at the end for float values
		double doubleValue = 32.4;
		
		System.out.println(Byte.MAX_VALUE);
		
	}
}
