class Frog{
	String name;
	int age;
	
	public void setName(String newName){
		name = newName;
	}
	
	public void setAge(int newAge){
		age = newAge;
	}
	
	public String getName(){
		return name;
	}
	
	public int getAge(){
		return age;
	}
}
public class Application {
	public static void main(String[] args) {
		Frog frog1 = new Frog();
		
		// frog1.name = "Vignesh";
		// frog1.age = 12;
		
		frog1.setName("Vigenesh");
		frog1.setAge(12);
		
		System.out.println(frog1.getName());
				
	}
}
